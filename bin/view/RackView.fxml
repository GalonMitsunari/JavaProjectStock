<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.RackController">
    <Label text="Gestion des Racks"/>
    <ListView fx:id="rackListView"/>
    <Label text="Référence"/>
    <TextField fx:id="rackReferenceField"/>
    <Label text="Capacité Max"/>
    <TextField fx:id="rackCapaciteMaxField"/>
    <Label text="Description"/>
    <TextField fx:id="rackDescriptionField"/>
    <Label text="Emplacement"/>
    <ListView fx:id="rackListView" />
    <Button text="Ajouter Rack" onAction="#addRack"/>
    <Button text="Mettre à Jour Rack" onAction="#updateRack"/>
    <Button text="Supprimer Rack" onAction="#deleteRack"/>
    <Button text="Retour" onAction="#goToMenu" />
</VBox>
